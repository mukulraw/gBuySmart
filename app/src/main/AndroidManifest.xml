<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.technuoma.gbuysmart">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="com.technuoma.gbuysmart.Bean"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name="com.technuoma.gbuysmart.Address" />
        <activity android:name="com.technuoma.gbuysmart.Web" />
        <activity
            android:name="com.technuoma.gbuysmart.OrderDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.OTP"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Location"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Search"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Orders"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Checkout"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Cart"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.SingleProduct"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Products"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.SubCat"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.gbuysmart.Spalsh"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.technuoma.gbuysmart.MainActivity" />
        <activity android:name="com.technuoma.gbuysmart.WebViewActivity" />

        <service
            android:name="com.technuoma.gbuysmart.MyFirebaseMessagingService"
            android:permission="">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/back" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />
        <!--
 Set custom default icon. This is used when no icon is set for incoming notification messages.
     See README(https://goo.gl/l4GJaQ) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" />
    </application>

</manifest>